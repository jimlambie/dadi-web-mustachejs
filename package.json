    "require-dir": "^0.3.2"
  },
  "devDependencies": {
    "colors": "^1.1.2",
    "env-test": "^1.0.0",
    "html-looks-like": "^1.0.2",
    "istanbul": "latest",
    "istanbul-cobertura-badger": "^1.1.0",
    "mocha": "~3.2.0",
    "object-path": "^0.11.4",
    "semantic-release": "^6.3.6",
